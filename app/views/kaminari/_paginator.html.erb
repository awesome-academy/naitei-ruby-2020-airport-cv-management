<%= paginator.render do -%>
  <nav class="pagination job-pagination mb-0 justify-content-center">
    <%= first_page_tag %>
    <% each_page do |page| -%>
      <% if page.display_tag? -%>
        <%= page_tag page %>
      <% elsif !page.was_truncated? -%>
        <%= gap_tag %>
      <% end -%>
    <% end -%>
    <%= last_page_tag %>
  </nav>
<% end -%>
